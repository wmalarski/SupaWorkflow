{
  "name": "supa-workflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "yarn install; lerna bootstrap;",
    "run:next": "lerna run --scope next-front-end --stream dev",
    "start": "run-p --print-label package:*",
    "build": "lerna run --parallel build",
    "test": "lerna run test --stream",
    "test:watch": "lerna run test:watch --stream",
    "test:coverage": "lerna run test:coverage --stream",
    "prepare": "husky install",
    "prettier": "prettier 'packages/**/*.(ts|tsx)' --write",
    "tsc": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.7",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@supabase/supabase-js": "^1.22.6",
    "cross-fetch": "^3.1.4",
    "downshift": "^6.1.7",
    "framer-motion": "^4.1.17",
    "nanoid": "^3.1.25",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-flow-renderer": "^9.6.7",
    "react-hook-form": "^7.15.3",
    "react-query": "^3.23.2",
    "replicache": "^7.0.0",
    "replicache-react": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@mswjs/data": "^0.5.1",
