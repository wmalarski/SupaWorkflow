    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "lerna": "^4.0.0",
    "lint-staged": "^11.1.2",
    "msw": "^0.35.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "4.4.3"
  }
